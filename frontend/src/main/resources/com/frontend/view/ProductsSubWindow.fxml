      <TextField fx:id="productsName" layoutX="14.0" layoutY="60.0" prefHeight="23.0" prefWidth="298.0" />
      <Label layoutX="14.0" layoutY="97.0" text="当前库存" />
      <TextField fx:id="productsReserveNow" layoutX="14.0" layoutY="113.0" prefHeight="23.0" prefWidth="298.0" />
