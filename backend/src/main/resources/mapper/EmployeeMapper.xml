<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.backend.mapper.EmployeeMapper">

    <resultMap id="BaseResultMap" type="com.backend.pojo.Employee">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="sex" column="sex" jdbcType="VARCHAR"/>
            <result property="phone" column="phone" jdbcType="INTEGER"/>
            <result property="hiredate" column="hiredate" jdbcType="DATE"/>
            <result property="birthday" column="birthday" jdbcType="DATE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,sex,
        phone,hiredate,birthday
    </sql>
</mapper>
